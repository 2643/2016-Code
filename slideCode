package org.usfirst.frc.team2643.robot;

import edu.wpi.first.wpilibj.command.Command;
import edu.wpi.first.wpilibj.command.Scheduler;
import edu.wpi.first.wpilibj.livewindow.LiveWindow;
import org.usfirst.frc.team2643.robot.commands.ExampleCommand;
import org.usfirst.frc.team2643.robot.subsystems.ExampleSubsystem;

        Talon linearSlide = new Talon(4);
        
        public static final ExampleSubsystem exampleSubsystem = new ExampleSubsystem();
        public static OI oi;
                oi = new OI();

    public void teleopPeriodic() {
        Scheduler.getInstance().run();
        
        if(Shooterstick.getRawButton(up)){
                //button port number update later
                linearSlide.set(0.2);
                linearSlide2.set(0.2);
        }
        else if(Shooterstick.getRawButton(down)){
                linearSlide.set(-0.2);
                linearSlide2.set(-0.2);
        }
        else{
                linearSlide.set(0.0);
                linearSlide2.set(0.0);
        }
    }
